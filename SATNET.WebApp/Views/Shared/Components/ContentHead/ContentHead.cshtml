@page
@model SATNET.WebApp.Models.MenuModel

@{

    <!-- Content Header (Page header) -->

    <div class="content-header">
        <div class="container-fluid">
            <div class="row mb-0">
                <div class="col-sm-6 ">
                        <h1 class="m-0 text-dark d-inline-flex mr-3"> @Html.DisplayFor(model => model.Heading)</h1>
                        <h6 class="m-0 text-dark d-inline-flex">@Html.DisplayFor(model => model.SubHeading)</h6>
                   
                </div><!-- /.col -->
                <div class="col-sm-6">
                    <ol class="breadcrumb float-sm-right">
                        <li class="breadcrumb-item"><a href="#"><span class='fa fa-plus' aria-hidden='true'></span>&nbsp;&nbsp;@Html.DisplayFor(model => model.Heading)</a></li>
                        <li class="breadcrumb-item active"> <span class='fa fa-archive' aria-hidden='true'></span>&nbsp;&nbsp; @Html.DisplayFor(model => model.SubHeading)</li>
                    </ol>
                </div><!-- /.col -->
            </div><!-- /.row -->
        </div><!-- /.container-fluid -->
    </div>
    <!-- /.content-header -->
    
}

@model SATNET.WebApp.Models.IP.IPViewModel
@*@inject SATNET.Domain.Enums.LookupTypes*@


    <form id="formAdd" method="post" data-ajax="true" asp-controller="IP" asp-action="Add" data-ajax-method="post" enctype="multipart/form-data" autocomplete="off">
        <div class="app-page-title">
            <div class="page-title-wrapper">
                <div class="page-title-heading">
                    <div>
                        Add IP
                        <div class="page-title-subheading">
                            Please fillout following form
                        </div>
                    </div>
                </div>
                <div class="page-title-actions">
                    <div class="d-inline-block dropdown">
                        <a href="@String.Format("/{0}", "IP/Index")" class=" btn btn-danger">
                            Cancel
                        </a>
                        <button type="submit" class="btn btn-default">Save Record</button>
                    </div>
                </div>
            </div>
        </div>

        <div class="row">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="col-lg-3">
                <div class="position-relative form-group">
                    <label class="lable-control">IP</label>
                    <input asp-for="Name" class="form-control" />
                    <span asp-validation-for="Name" class="text-danger"></span>
                </div>
            </div>
        </div>
    </form>


@section scripts{
    @await Html.PartialAsync("/Views/Shared/_ValidationScriptsPartial.cshtml")
}

